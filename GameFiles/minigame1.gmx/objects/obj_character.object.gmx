          <argument>
            <kind>1</kind>
            <string>///Movement
var up,left,down,right;
 
up = keyboard_check(vk_up);
left = keyboard_check(vk_left);
down = keyboard_check(vk_down);
right = keyboard_check(vk_right);
 
speed = move_speed;
 
if(up &amp;&amp; left &amp;&amp; down &amp;&amp; right){
    speed = move_speed;
}
else if(up &amp;&amp; left &amp;&amp; down){
    direction = 180;
}
else if(up &amp;&amp; left &amp;&amp; right){
    direction = 90;
}
else if(up &amp;&amp; down &amp;&amp; right){
    direction = 0;
}
else if(left &amp;&amp; down &amp;&amp; right){
    direction = 270;
}
else if(up &amp;&amp; left){
    direction = 135;
}
else if(up &amp;&amp; down){
    speed = 0;
}
else if(up &amp;&amp; right){
    direction = 45;
}
else if(left &amp;&amp; down){
    direction = 225;
}
else if(left &amp;&amp; right){
    speed = 0;
}
else if(down &amp;&amp; right){
    direction = 315;
}
else if(up){
    direction = 90;
}
else if(left){
    direction = 180;
}
else if(down){
    direction = 270;
}
else if(right){
    direction = 0;
}
else{
    speed = 0;
}

// Press space to slow down
if keyboard_check_pressed(vk_space)
{
    move_speed = move_speed / 1.5;
}

if keyboard_check_released(vk_space)
{
    move_speed = move_speed * 1.5;
}

//Collisions
if (right)
{
if (place_meeting(x+move_speed,y,obj_wall))
{
    hspeed = 0;
}
}

if (left)
{
if (place_meeting(x-move_speed,y,obj_wall))
{
    hspeed = 0;
}
}

if (down)
{
if (place_meeting(x,y+move_speed,obj_wall))
{
    vspeed = 0;
}
}

if (up)
{
if (place_meeting(x,y-move_speed,obj_wall))
{
    vspeed = 0;
}
}

//collision with destroyed wall

if (right)
{
if (place_meeting(x+move_speed,y,obj_wall_destroy))
{
    hspeed = 0;
}
}

if (left)
{
if (place_meeting(x-move_speed,y,obj_wall_destroy))
{
    hspeed = 0;
}
}

if (down)
{
if (place_meeting(x,y+move_speed,obj_wall_destroy))
{
    vspeed = 0;
}
}

if (up)
{
if (place_meeting(x,y-move_speed,obj_wall_destroy))
{
    vspeed = 0;
}
}


</string>
